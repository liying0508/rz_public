<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">
<mapper namespace="io.renren.modules.financing.dao.FDictDao">
    <resultMap id="BaseResultMap" type="io.renren.modules.financing.dto.FDictDTO">
        <id column="id" jdbcType="BIGINT" property="id" />
        <result column="dict_name" jdbcType="BIGINT" property="dictName" />
        <result column="dict_key" jdbcType="VARCHAR" property="dictKey" />
        <result column="dict_value" jdbcType="VARCHAR" property="dictValue" />
        <result column="dict_module" jdbcType="VARCHAR" property="dictModule" />
    </resultMap>
    <select id="selectModuleAllKey" resultType="io.renren.modules.financing.dto.FDictDTO">
        SELECT * FROM
        f_dict
        where dict_module = #{dictModule}
    </select>
</mapper>