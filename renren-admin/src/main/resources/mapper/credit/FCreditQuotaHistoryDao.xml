<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE mapper PUBLIC "-//mybatis.org//DTD Mapper 3.0//EN" "http://mybatis.org/dtd/mybatis-3-mapper.dtd">

<mapper namespace="io.renren.modules.credit.dao.FCreditQuotaHistoryDao">

    <resultMap type="io.renren.modules.credit.entity.FCreditQuotaHistoryEntity" id="fCreditQuotaHistoryMap">
        <result property="id" column="id"/>
        <result property="creditQuotaDirect" column="credit_quota_direct"/>
        <result property="creditQuotaId" column="credit_quota_id"/>
        <result property="creditQuotaIndirect" column="credit_quota_indirect"/>
        <result property="usedQuotaDirect" column="used_quota_direct"/>
        <result property="usedQuotaIndirect" column="used_quota_indirect"/>
        <result property="creator" column="creator"/>
        <result property="createDate" column="create_date"/>
    </resultMap>
    <select id="selectAllByCreditQuotaId"
            resultType="io.renren.modules.credit.dto.FCreditQuotaHistoryDTO">
        SELECT * FROM
        f_credit_quota_history
        WHERE credit_quota_id = #{creditQuotaId}
    </select>
</mapper>